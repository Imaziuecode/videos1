        p.content !== undefined && (this.content = p.content);
        p.text !== undefined && (this.text = p.text);
        p.id !== undefined && (this.id = p.id);
        p.cssClass !== undefined && (this.cssClass = p.cssClass);
        p.position !== undefined && (this.position = p.position);
        p.css !== undefined && (this.css = p.css);
    }


    create(p) {
        var e;
        this.parent == undefined && this.position !== undefined && (this.parent = null);
        if (this.parent !== undefined) {
            if (this.element !== undefined) {
                e = document.createElement(this.element);
                if (this.attributes !== undefined) {
                    if (this.attributes) {
                        for (var attr in (this.attributes)) {
                            e.setAttribute(attr, this.attributes[attr]);
                        }
                    }
                }

                if (this.id !== undefined) {
                    if (this.id) {
                        e.setAttribute("id", this.id);
                    }
                }
                if (this.content !== undefined) {
                    if (this.content) {
                        e.innerHTML = this.content;
                    }
                }
                if (this.text !== undefined) {
                    if (this.text) {
                        e.innerText = this.text;
                    }
                }

                if (this.cssClass !== undefined) {
                    if (this.cssClass) {

                        e.className += " " + this.cssClass + " ";


                    }
                }
                if (this.css !== undefined) {
                    if (this.css) {
                        e.style = this.css;
                    }
                }

                if (this.position) {
                    switch (this.position) {
                        case "before":
                            (this.parent).before(e);
                            break;
                        case "prepend":
                            (this.parent).prepend(e);
                            break;
                        case "after":
                            (this.parent).after(e);
                            break;
                        default:
